# Rugs Feed Service Environment Variables
# Copy to .env and customize as needed

# Backend Connection
RUGS_BACKEND_URL=https://backend.rugs.fun

# API Server
PORT=9016
HOST=0.0.0.0

# Storage
DATA_PATH=./data
STORAGE_PATH=/data/rugs_feed.db

# Reconnection
MAX_RECONNECT_ATTEMPTS=100
RECONNECT_DELAY=5

# Logging
LOG_LEVEL=INFO
